{
  "routes": [
    {
      "method": "POST",
      "path": "/password",
      "handler": "password.index"
    },
    {
      "method": "GET",
      "path": "/password/user_state/:identifier",
      "handler": "password.findUserByEmail"
    },
    {
      "method": "PUT",
      "path": "/password/newpassword",
      "handler": "password.resetPassword"
    }
  ]
}
